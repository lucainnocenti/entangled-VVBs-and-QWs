@article {Bauer2015,
	author = {Bauer, Thomas and Banzer, Peter and Karimi, Ebrahim and Orlov, Sergej and Rubano, Andrea and Marrucci, Lorenzo and Santamato, Enrico and Boyd, Robert W. and Leuchs, Gerd},
	title = {Observation of optical polarization M{\"o}bius strips},
	volume = {347},
	number = {6225},
	pages = {964--966},
	year = {2015},
	doi = {10.1126/science.1260635},
	publisher = {American Association for the Advancement of Science},
	abstract = {M{\"o}bius strips are three-dimensional structures consisting of a surface with just a single side. Readily demonstrated by snipping a paper ring, adding a twist, and then joining the ends of paper together again, these structures have intriguing mathematical properties in terms of topology and geometry. Bauer et al. used a liquid crystal to engineer the wavefront of a laser beam to make an optical version of the M{\"o}bius strip by effectively {\textquotedblleft}snipping and twisting{\textquotedblright} the polarization properties of the light beam.Science, this issue p. 964 M{\"o}bius strips are three-dimensional geometrical structures, fascinating for their peculiar property of being surfaces with only one {\textquotedblleft}side{\textquotedblright}{\textemdash}or, more technically, being {\textquotedblleft}nonorientable{\textquotedblright} surfaces. Despite being easily realized artificially, the spontaneous emergence of these structures in nature is exceedingly rare. Here, we generate M{\"o}bius strips of optical polarization by tightly focusing the light beam emerging from a q-plate, a liquid crystal device that modifies the polarization of light in a space-variant manner. Using a recently developed method for the three-dimensional nanotomography of optical vector fields, we fully reconstruct the light polarization structure in the focal region, confirming the appearance of M{\"o}bius polarization structures. The preparation of such structured light modes may be important for complex light beam engineering and optical micro- and nanofabrication.},
	issn = {0036-8075},
	journal = {Science}
}

