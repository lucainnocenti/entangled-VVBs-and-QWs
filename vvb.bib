	Pmid = {24067608},
	Title = {Microscopic observation of magnon bound states and their dynamics},
	Url = {http://dx.doi.org/10.1038/nature12541},
	Volume = {502},
	Year = {2013},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nature12541}}

@article{tulsi2008faster,
	Abstract = {We consider the problem of finding a desired item out of N items arranged on the sites of a two-dimensional lattice of size NxN. The previous quantum-walk based algorithms take O(N ln N) steps to solve this problem, and it is an open question whether the performance can be improved. We present an algorithm which solves the problem in O(N ln N) steps, thus giving an O(ln N) improvement over the known algorithms. The improvement is achieved by controlling the quantum walk on the lattice using an ancilla qubit.},
	Author = {Tulsi, Avatar},
